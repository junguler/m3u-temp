<script>
    var streams = ["\/radio_stations\/stations\/play-stream?id=361721"];
    var realStreams = {"\/radio_stations\/stations\/play-stream?id=361721":"http:\/\/aceradio.net\/itunes\/hitz.pls"};
        document.addEventListener("DOMContentLoaded", function (event) {
        callMagnificPopup();
        storeStationId();
    });

        function storeStationId() {
        var stationId = $('[data-name=station-wrapper]').data('station-id');
        if (stationId) {
            commonFrontend.storeHistory(stationId);
        }
    }

    function callMagnificPopup() {
        $('.call-update').magnificPopup({
            type: 'inline',
            fixedContentPos: false,
            fixedBgPos: true,
            overflowY: 'hidden',
            closeBtnInside: true,
            preloader: false,
            midClick: true,
            removalDelay: 300,
            mainClass: 'my-mfp-zoom-in'
        });
    }
</script>
