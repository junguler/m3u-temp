 			 <td width="745" height="5" align="left" valign="middle" style="background-color: white;">

                  
                          <div style="float: left; margin: 10px;">
                            
                                <img src="https://item.radio456.com/007452/330159/logo-85118.png" />
                                
                                <script type="text/javascript">
                                    var noRecursions = 0;
                                    $(document).ready(function () {
                                        $("#jquery_jplayer_1").jPlayer({
                                            ready: function () {
                                                noRecursions++;
                                                $(this).jPlayer("setMedia", {
                                                    title: "Zupanijski Radio Virovitica",
                                                    mp3: "http://165.22.31.76:9300/stream"
                                                }).jPlayer("play");
                                            },
                                            error: function (event) {
                                                var lCoverage = "";
                                                if (lCoverage.length == 2 && lCoverage != "US") {
                                                    $(this).jPlayer("setMedia", {
                                                        title: "Zupanijski Radio Virovitica",
                                                        mp3: "http://primary4.vtuner.com/setupapp/audio/For%20geoblocked-%20English.mp3"
                                                    }).jPlayer("play");
                                                }
                                                else if (noRecursions == 1 && event.jPlayer.error.type === "e_url") {
                                                    noRecursions++;
                                                    // Try to handle SHOUTcast.
                                                    var rawUrl = "http://165.22.31.76:9300/stream";
                                                    if (rawUrl.endsWith("/")) rawUrl += ";";
                                                    else if (!rawUrl.endsWith(";")) rawUrl += "/;";
                                                    $(this).jPlayer("setMedia", {
                                                        title: "Zupanijski Radio Virovitica",
                                                        mp3: rawUrl
                                                    }).jPlayer("play");
                                                }
                                                else if (noRecursions == 2 && event.jPlayer.error.type === "e_url") {
                                                    noRecursions++;
                                                    // Try to handle Icecast.
                                                    var rawUrl = "http://165.22.31.76:9300/stream";
                                                    if (rawUrl.endsWith("/")) rawUrl += ";stream/1";
                                                    else if (!rawUrl.endsWith(";")) rawUrl += "/;stream/1";
                                                    $(this).jPlayer("setMedia", {
                                                        title: "Zupanijski Radio Virovitica",
                                                        mp3: rawUrl
                                                    }).jPlayer("play");
                                                }
                                                else if (noRecursions == 3 && event.jPlayer.error.type === "e_url") {
                                                    noRecursions++;
                                                    $(this).jPlayer("setMedia", {
                                                        title: "Zupanijski Radio Virovitica",
                                                        mp3: "http://primary4.vtuner.com/setupapp/audio/err.mp3"
                                                    }).jPlayer("play");
                                                }
                                            },
                                            playing: function () {
                                                $("#jp_container_1").show();
                                            },
                                            cssSelectorAncestor: "#jp_container_1",
                                            swfPath: "js",
                                            supplied: "mp3",
                                            useStateClassSkin: true,
                                            autoBlur: false,
                                            smoothPlayBar: true,
                                            keyEnabled: true,
                                            remainingDuration: true,
                                            toggleDuration: true,
                                            loop: true
                                        });
                                    });
                              </script>
                        
