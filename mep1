	window.mss = false;
	var has_focus = true;
	function loading_time() {
    $(":focus").each(function() {
      if($(this).attr("id")=="iframeID") { 
      	has_focus = true;
      	console.log('focus true');
      }
    });
		
    setTimeout("loading_time()", 2000);
	}
var btimer = 0;

