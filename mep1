<section class="review">

<h2>TDI Acoustic komentara</h2>


	<form class="review__form post_form" action="https://onlineradiobox.com/rs/tdiacoustic/" anchor="review" id="reviewForm">
	
		<input type="button" class="button" value="Ostavite svoju ocenu" data-target="#popup_auth" data-toggle="modal" />
	
	</form>
	<script type="0739f00523ed2000a07d52b2-text/javascript">
	function confirmDeleleteReview(e, text) {
		if (!confirm(text)) {
			e.stopImmediatePropagation();
		}
	}

	function checkReviewForm() {
		var form = document.getElementById("reviewForm");
		var submit = form.querySelector("input[type=submit]");
		if (!submit) {
				return;
		}
		var review = form.querySelector("textarea[name=review]");
		var ratings = form.querySelectorAll("input[name=review_rating]");
		var checked = false;
		for (var i = 0; i < ratings.length; i++) {
			if (ratings[i].checked) {
				checked = true;
				break;
			}
		}
		if (review.value=="" || !checked) {
			submit.disabled = true;
		} else {
			submit.disabled = false;
		}
	}
	checkReviewForm();
	</script>



<!-- /reviews -->
</section>

