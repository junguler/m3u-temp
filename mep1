                let datos = {
                    'palabra': palabra,
                };
                if(palabra.length>2)
                {
                    $.ajax({
                        url: 'https://escuchar.radio/buscarRadioAjax',
                        headers: {
                            'X-CSRF-TOKEN': $('meta[name="csrf-token"]').attr('content')
                        },
                        type: 'post',
                        data: {
                            datos: datos
                        },
                        dataType: 'json',
                        success: function (data)
                        {
                            if (data.exito)
                            {
                                let capa='';
                                if(data.radios.length>0)
                                {
                                    data.radios.forEach(function(radio){
                                        capa+=
                                            '<div class="col-xl-2 col-md-4 col-6">'+
                                                '<div class="custom-card mb-3">'+
                                                    '<a href="/'+radio.slug+'" class="text-dark" title="Emisora '+radio.nombre+'">'+
                                                        '<img width="175" src="https://escuchar.radio/assets/images/radios/'+radio.slug+'.jpg" alt="'+radio.nombre+'" class="card-img--radius-md">'+
                                                            '<p class="text-truncate mt-2">'+radio.nombre+'</p>'+
                                                    '</a>'+
                                                '</div>'+
                                            '</div>';
                                    });
                                    $("#resultadosBusquedaRadio").html(capa);
                                }
                                else
                                {
                                    $("#resultadosBusquedaRadio").html('<div class="col-md-6 text-white">No hemos encontrado resultados</div>');
                                }

                                if(data.generos.length>0)
                                {
                                    capa = '';
                                    data.generos.forEach(function(genero){
                                        capa+=
                                            '<div class="col-xl-2 col-md-4 col-6">'+
                                                '<div class="custom-card mb-3">'+
                                                    '<a href="/genero/'+genero.slug+'" class="text-dark" title="Emisoras de '+genero.nombre+'">'+
                                                        '<img src="'+genero.logo+'" alt="'+genero.nombre+'" class="card-img--radius-md">'+
                                                        '<p class="text-truncate mt-2">'+genero.nombre+'</p>'+
                                                    '</a>'+
                                                '</div>'+
                                            '</div>';
                                    });
                                    $("#busquedaGeneros").show();
                                    $("#resultadosBusquedaGeneros").html(capa);
                                }
                                else
                                {
                                    $("#busquedaGeneros").hide();
                                }

                                if(data.ciudades.length>0)
                                {
                                    capa = '';
                                    data.ciudades.forEach(function(ciudad){
                                        capa+=
                                            '<div class="col-xl-2 col-md-4 col-6">'+
                                                '<div class="custom-card mb-3">'+
                                                    '<a href="/ciudad/'+ciudad.slug+'" class="text-dark" title="Emisoras de '+ciudad.nombre+'">'+
                                                        '<p class="text-truncate mt-2">'+ciudad.nombre+'</p>'+
                                                    '</a>'+
                                                '</div>'+
                                            '</div>';
                                    });
                                    $("#busquedaCiudades").show();
                                    $("#resultadosBusquedaCiudades").html(capa);
                                }
                                else
                                {
                                    $("#busquedaCiudades").hide();
                                }

                                if(data.regiones.length>0)
                                {
                                    capa = '';
                                    data.regiones.forEach(function(region){
                                        capa+=
                                            '<div class="col-xl-2 col-md-4 col-6">'+
                                                '<div class="custom-card mb-3">'+
                                                    '<a href="/region/'+region.slug+'" class="text-dark" title="Emisoras de '+region.nombre+'">'+
                                                        '<p class="text-truncate mt-2">'+region.nombre+'</p>'+
                                                    '</a>'+
                                                '</div>'+
                                            '</div>';
                                    });
                                    $("#busquedaRegiones").show();
                                    $("#resultadosBusquedaRegiones").html(capa);
                                }
                                else
                                {
                                    $("#busquedaRegiones").hide();
                                }

                                if(data.paises.length>0)
                                {
                                    capa = '';
                                    data.paises.forEach(function(pais){
                                        capa+=
                                            '<div class="col-xl-2 col-md-4 col-6">'+
