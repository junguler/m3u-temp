            function ponerRadio(ponerAnuncio = false)
            {
                let audio = videojs('audio');
                audio.src({src: url});

                if(ponerAnuncio) //Si tiene anuncio escuchamos primero el anuncio y luego reproducimos la radio desmuteada
                {
                    //Obtenemos el audio del anuncio
                    let anuncio = document.getElementById("anuncio");
                    anuncio.pause();
                    anuncio.currentTime=0;
                    anuncio.muted = false;
                    anuncio.play();

                    //Reproducimos la radio muteada hasta que termine el anuncio
                    audio.muted(true);
                    audio.play();
                    setTimeout(function(){
                        audio.muted(false); //Quitamos el mute a la radio
                    },anuncio.duration*1000);
                }
                else //Si no tiene anuncio simplemente reproducimos la radio tal cual
                {
                    audio.play();
                }

                /*
                //Si es la primera vez que reproducimos la radio, activamos los anuncios
                if(primeraReproduccion)
                {
                    primeraReproduccion = false;
                    setTimeout(ponerAnuncio, 150000);
                    setInterval(ponerAnuncio, 1800000);
                }
                 */
            }

            function inicializarBuscador()
            {
                let $search = $('#searchForm #searchInput');

                $search.on('click', function (e) {
                    e.stopPropagation();
                    $body.addClass('open-search');
                    $headerBackdrop.addClass('show');
                });

                $body.on('click', function () {
                    $body.removeClass('open-search');
                    $headerBackdrop.removeClass('show');
                });
            }

            function buttonClicks()
            {
                $('#toggleSidebar').on('click', function () {
                    $body.toggleClass('iconic-sidebar');
                });

                $('#openSidebar').on('click', function (e) {
                    e.stopPropagation();
                    $body.removeClass('open-search');
                    $body.addClass('open-sidebar');
                    $sidebarBackdrop.addClass('show');
                    $headerBackdrop.removeClass('show');
                });

                $('#hideSidebar').on('click', function () {
                    $body.removeClass('open-sidebar');
                    $sidebarBackdrop.removeClass('show');
                });

                $('#playList').on('click', function () {
                    $body.toggleClass('open-right-sidebar');
                });
            }

