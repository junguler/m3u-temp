<script>

    $(document).ready(function() {
        $.raty.path = "/libs/jquery-raty/images";
        $('span.comment-rating').raty({ readOnly: true });
        $('span#rateInputId').raty({ scoreName: 'score' });

        $(document).on("keydown", ":input:not(textarea)", function(event) {
            return event.key != "Enter";
        });
        $('#reviewFormId').on("submit", function(e) {
            e.preventDefault();
            console.log('hi');
        });

        $('#reviewFormSubmitId').on("click", function() {
            var data = $('#reviewFormId').serialize();
            if(!data.score)
                data.score = 5;
            $.ajax({
                type: "POST",
                url: "/review/create",
                data:  data,
                beforeSend: function() {
                    $('#commentInfoBox').empty();
                },
                success: function(response) {
                    $('#commentInfoBox').text("Your comment will be published after approvement!").remove('d-none');
                    $('#reviewFormId').hide();
                },
                error: function(response) {
                    $('#commentInfoBox').text("An error occured, please check your data and try again later!").remove('d-none');
                }
            });

        });
    });


</script>
