<script>
$('#menu-opener').on('click', function(){
    if ($('#menu-content').hasClass("d-none")) {
    $('#menu-content').addClass('d-block');
    $('#menu-content').removeClass('d-none');
    } else {
    $('#menu-content').addClass('d-none');
    $('#menu-content').removeClass('d-block');
 }
});
</script>


    
