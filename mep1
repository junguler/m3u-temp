<script src="https://cdnjs.cloudflare.com/ajax/libs/jquery.form/4.3.0/jquery.form.min.js" integrity="sha384-qlmct0AOBiA2VPZkMY3+2WqkHtIQ9lSdAsAn5RUJD/3vA5MKDgSGcdmIv4ycVxyn" crossorigin="anonymous"></script>

        <script>
var aud = new Audio();

$(document).on('click', '.play' , function(e){
    let url = $(this).attr('url');
    
    if(url.includes('vobook.ru')){
        aud.crossOrigin = 'crossorigin'
    }
    

    playURL(url);
})

$(document).on('click', '.pause' , function(e){
    pause();
    $('.pause').html('<img src="assets/image/play.png" alt="Play" title="Play" width="64" height="64">');
    $('.pause').attr('class','play');
    return false;
})

aud.onerror = function () {
  if( aud.src == "data:audio/wav;base64,UklGRiQAAABXQVZFZm10IBAAAAABAAEAVFYAAFRWAAABAAgAZGF0YQAAAAA=") return false;
  
  myErrHandler('Ошибка плеера ' + aud.error.code, window.location.href, 0, 0, aud.error, 'player')
  
  
  switch (aud.error.code) {
     case aud.error.MEDIA_ERR_ABORTED:
       alert('Playback interrupted by user.');
       break;
       
     case aud.error.MEDIA_ERR_NETWORK:
       alert('Network error, loading media file is stopped.');
       break;
       
     case aud.error.MEDIA_ERR_DECODE:
       alert('The playback of the media file is interrupted due to the presence in it of elements which cannot be decoded by the browser.');
       break;
       
     case aud.error.MEDIA_ERR_SRC_NOT_SUPPORTED:
       alert('Media file can not be downloaded because of a server error or a network, or the media format is not supported.');
       break;
       
     default:
       alert('Not a known bug.');
       break;
   }
   
    $('.load').html('<img src="assets/image/play.png" alt="Play" title="Play" width="64" height="64">');
    $('.load').attr('class','play');
   
}
aud.onemptied = function () {
    $('.pause').html('<img src="assets/image/load.gif" width="64" height="64">');
    $('.pause').attr('class','load');
}
aud.oncanplaythrough = function () {
    $('.load').html('<img src="assets/image/pause.png" alt="Pause" title="Pause" width="64" height="64">');
    $('.load').attr('class','pause');
}
aud.onplaying = function () {
    $('.load').html('<img src="assets/image/pause.png" alt="Pause" title="Pause" width="64" height="64">');
    $('.load').attr('class','pause');
}
aud.onplay = function () {
    $('.play').html('<img src="assets/image/load.gif" width="64" height="64">');
    $('.play').attr('class','load');
}

function range($this){
    volume = $this.value;
	aud.volume = volume;
	$('.range').css({'background':'-webkit-linear-gradient(left ,#767676 0%,#767676 '+volume*100+'%,#fff '+volume*100+'%, #fff 100%)'});
	if(aud.muted){
        aud.muted = false;
        $('.img-mute').attr('src','assets/image/stereo.png');
        $('.img-mute').attr('class','img-stereo');
	}
 }
 
 $(document).on('click', '.img-stereo' , function(e){
    aud.muted = true;
    $('.img-stereo').attr('src','assets/image/mute.png');
    $('.img-stereo').attr('class','img-mute');
    return false;
})
$(document).on('click', '.img-mute' , function(e){
    aud.muted = false;
    $('.img-mute').attr('src','assets/image/stereo.png');
    $('.img-mute').attr('class','img-stereo');
    return false;
})
function playURL(url){
    stop();
    aud.src = url;
    play();
    aud.volume = document.querySelector('input[type=range]').value;
}
function play(){
    aud.load();
    aud.play();
}
function pause(){
    aud.pause();
    aud.src = 'data:audio/wav;base64,UklGRiQAAABXQVZFZm10IBAAAAABAAEAVFYAAFRWAAABAAgAZGF0YQAAAAA=';
}
function stop(){
    pause();
}
</script>


<script>
$(document).ready(function() {
        /*Correspondence Slider*/
        $('.carousel-correspondence .item').each(function() {
            var itemToClone = $(this);

            for (var i = 1; i < 6; i++) {
                itemToClone = itemToClone.next();
                if (!itemToClone.length) {
                    itemToClone = $(this).siblings(':first');
                }
                itemToClone.children(':first-child').clone()
                    .addClass("cloneditem-" + (i))
                    .appendTo($(this));
            }
        });
        /*Correspondence Slider*/
    });
    
(function(d, s, id) {
  var js, fjs = d.getElementsByTagName(s)[0];
  if (d.getElementById(id)) return;
  js = d.createElement(s); js.id = id;
  js.src = 'https://connect.facebook.net/en_US/sdk.js#xfbml=1&version=v3.0';
  fjs.parentNode.insertBefore(js, fjs);
}(document, 'script', 'facebook-jssdk'));
</script>  
