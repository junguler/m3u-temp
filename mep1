		var p_has_focus = has_focus; 
    has_focus=true;  
    console.log('focus true');
    clearInterval(window.btimers);
    if (window.mss = true && p_has_focus == false && btimer > 60) {
    	googletag.pubads().refresh([window.bnrHeader, window.bnrSag]);
    	window.mss = false;
    	btimer = 0;
  	}
}

