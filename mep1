            function ajustarMenu()
            {
                let alturaSidebar = $("#sidebar").outerHeight();
                let alturaAudioPlayer = window.matchMedia("(max-width: 768px)").matches ? 10 : $("#audioPlayer").outerHeight()+10;
                let alturaNavbarBottom = $(".navbar-bottom").outerHeight();
                let alturaSidebarHeader = $(".sidebar-header").outerHeight();

                $(".navbar").css("height",(alturaSidebar-alturaAudioPlayer-alturaNavbarBottom-alturaSidebarHeader));
            }

