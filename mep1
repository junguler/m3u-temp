 <script>

 var radioSt = 'eff5be9b-bde2-4113-bfce-b98862d95f00';
 $(document).ready(function(){

	 $(document).on('click', 'a', function (e) {
		    if ($(this).attr('href') == '#') {
		        e.preventDefault();
		    }
		});

	 $("#radioicon").on('error', function(){
			$('#radioicon').remove();
			});


 	$('#playradio').on('play', function () {
 		$('#votespot').removeClass('d-none');
 		$.post("clickCount.php", {station:radioSt}, 
 	    	    function(data,status)
 	    	    {
 		
 			console.log(data);
 		     return true; 
 	    	    });

 		});
 	
 	});
 $('#imvoting').on('click', function () {

 	$.post("vote.php", {station:radioSt}, 
     	    function(data,status)
     	    {
          console.log(data);
 		var dataa = $.parseJSON(data);
 		if(dataa.ok){
 			$('#myth').removeClass('text-warning').addClass('text-success');
 			$('#imvoting').text("Voted!!!");
 			var vtt = parseInt($('.vtt').text());
 			$('.vtt').text(vtt+1);
 			$('#votepos').html('<span class="text-success">'+dataa.message+'</span>');
 		} else {
 			$('#votepos').html('<span class="text-success">'+dataa.message+'</span>');
 		}
 		console.log('ST '+ status + ' DT ' +data + ' x '+ dataa.message);
 	     return true; 
     	    });
 	
 	

 });

</script>
