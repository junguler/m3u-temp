        function actualizarCanciones()
        {
            let datos = {
                'radio_id': 29099,
            };
            $.ajax({
                url: 'https://escuchar.radio/actualizarCancionesRadioAjax',
                headers: {
                    'X-CSRF-TOKEN': $('meta[name="csrf-token"]').attr('content')
                },
                type: 'post',
                data: {
                    datos: datos
                },
                dataType: 'json',
                success: function (data)
                {
                    if (data.exito)
                    {
                        let canciones = '';
                        data.canciones.forEach(function(cancion){
                            let foto = '';
                            if(cancion.foto!=null)
                            {
                                foto = '<img src="'+cancion.foto+'" alt="'+(cancion.titulo ? cancion.titulo : cancion.cancion)+'" class="card-img--radius-sm">';
                            }
                            let artista = '';
                            if(cancion.artista)
                            {
                                artista = '<p class="text-truncate text-muted font-sm">'+cancion.artista+'</p>';
                            }
                            let date= new Date(cancion.created_at);
                            let horas = (date.getHours()<10?'0':'') + date.getHours();
                            let minutos = (date.getMinutes()<10?'0':'') + date.getMinutes();
                            let fecha = horas+":"+minutos;
                            canciones+=
                                '<div class="custom-list--item">' +
                                    '<div class="text-dark custom-card--inline">' +
                                        '<div class="custom-card--inline-img">' +
                                            foto+
                                        '</div>' +
                                        '<div class="custom-card--inline-desc">' +
                                            '<p class="text-truncate mb-0">'+(cancion.titulo ? cancion.titulo : cancion.cancion)+'</p>' +
                                            artista+
                                        '</div>' +
                                    '</div>' +
                                    '<div class="text-center" style="flex: 0 0 22%;">' +
                                        fecha+
                                    '</div>' +
                                '</div>';
                        });
                        $("#canciones").html(canciones);
                    }
                },
                error: function ()
                {
                    console.log("ERROR");
                }
            });
        }

        function enviarComentario()
        {
            let comentario = $("#comentario").val().trim();
            let datos = {
                'radio_id': 29099,
                'comentario': comentario,
                'valoracion': isNaN(parseInt($(".valoracion-activa").last().attr("data-id"))) ? 5 : parseInt($(".valoracion-activa").last().attr("data-id")),
            };
            $.ajax({
                url: 'https://escuchar.radio/nuevoComentarioAjax',
                headers: {
                    'X-CSRF-TOKEN': $('meta[name="csrf-token"]').attr('content')
                },
                type: 'post',
                data: {
                    datos: datos
                },
                dataType: 'json',
                success: function (data)
                {
                    if (data.exito)
                    {
                        actualizarComentarios();
                        $("#comentario").val("");
                    }
                },
                error: function ()
                {
                    console.log("ERROR");
                }
            });
        }

        function actualizarComentarios()
        {
            let datos = {
                'radio_id': 29099
            };
            $.ajax({
                url: 'https://escuchar.radio/actualizarComentariosAjax',
                headers: {
                    'X-CSRF-TOKEN': $('meta[name="csrf-token"]').attr('content')
                },
                type: 'post',
                data: {
                    datos: datos
                },
                dataType: 'json',
                success: function (data)
                {
                    if (data.exito)
                    {
                        console.log(data);
                        let comentarios = '';
                        data.comentarios.forEach(function(comentario){
                            let date= new Date(comentario.created_at);
                            let dia = (date.getDate()<10 ? '0':'') + date.getDate();
                            let mes = ((date.getMonth()+1)<10 ? '0':'') + (date.getMonth()+1);
                            let a√±o = date.getFullYear();
                            let fecha = dia+"/"+mes+"/"+a√±o;

                            let estrellas = '';
                            for(let i =1; i<=comentario.valoracion; i++)
                            {
                                estrellas += '<img width="15" height="15" data-id="1" src="https://escuchar.radio/assets/images/iconos/estrella-amarilla.svg" alt="Puntuaci√≥n '+i+'">';
                            }
                            if(comentario.valoracion<5)
                            {
                                for(let i=parseInt(comentario.valoracion)+1; i<=5; i++)
                                {
                                    estrellas += '<img width="15" height="15" data-id="1" src="https://escuchar.radio/assets/images/iconos/estrella-hueca.svg" alt="Puntuaci√≥n '+i+'">';
                                }
                            }

                            comentarios+=
                                '<div class="comentario mt-3">' +
                                    '<div class="media">' +
                                        '<img class="mr-3" src="https://escuchar.radio/assets/images/iconos/chico.png" alt="'+comentario.nombre_usuario+'">'+
                                        '<div class="media-body">' +
                                            '<div class="autor">' +
                                                estrellas +
                                                '<strong class="enlaces">'+comentario.nombre_usuario+'</strong>'+
                                            '</div>' +
                                            '<div class="fechaComentario">' +
                                                fecha+
                                            '</div>' +
                                            '<div class="textoComentario">' +
                                                comentario.texto+
                                            '</div>' +
                                        '</div>' +
                                    '</div>' +
                                '</div>';
                        });
                        $("#comentarios").html(comentarios);
                    }
                },
                error: function ()
                {
                    console.log("ERROR");
                }
            });
        }

        function comprobarUsuarioLogeado()
        {
            $.ajax({
                url: 'https://escuchar.radio/comprobarUsuarioLogeado',
                headers: {
                    'X-CSRF-TOKEN': $('meta[name="csrf-token"]').attr('content')
                },
                type: 'post',
                dataType: 'json',
                success: function (data)
                {
                    if (data.logeado)
                    {
                        $("#enviarComentario").html('<button class="btn btn-warning mt-1" onclick="enviarComentario()">Enviar</button>');
                    }
                    else
                    {
                        let capaRRSS=
                            '<div class="logearse mt-2">'+
                                '<a href="https://escuchar.radio/sign-in/facebook"><img src="https://escuchar.radio/assets/images/iconos/facebook.png" alt="Login con Facebook"></a>'+
                                '<a href="https://escuchar.radio/sign-in/twitter"><img src="https://escuchar.radio/assets/images/iconos/twitter.png" alt="Login con Twitter"></a>'+
                                '<a href="https://escuchar.radio/sign-in/google"><img src="https://escuchar.radio/assets/images/iconos/google.png" alt="Login con Google"></a>'+
                            '</div>';
                        $("#enviarComentario").html(capaRRSS);
                    }
                },
                error: function ()
                {
                    console.log("ERROR");
                }
            });
        }

        function valoracionHover()
        {
            //console.log($(this).attr("data-id"));
            let actual = $(this).attr("data-id");
            for(let i=1; i<=actual; i++)
            {
                $("#star-rating img[data-id="+i+"]").attr("src","https://escuchar.radio/assets/images/iconos/estrella-amarilla.svg");
                $("#star-rating img[data-id="+i+"]").addClass('valoracion-activa');
            }
            actual++;
            for(let i=actual; i<=5; i++)
            {
                $("#star-rating img[data-id="+i+"]").attr("src","https://escuchar.radio/assets/images/iconos/estrella-hueca.svg");
                $("#star-rating img[data-id="+i+"]").removeClass('valoracion-activa');
            }
        }

